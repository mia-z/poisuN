{
  "name": "@miaz/poisun",
  "version": "0.0.1",
  "description": "Typescript REST API wrapper for SpaceTradersAPI game",
  "main": "index.ts",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": {
        "default": "./index.ts"
      }
    }
  },
  "repository": "https://github.com/mia-z/poisun",
  "author": "Ryan",
  "license": "MIT",
  "packageManager": "yarn@3.5.1",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.60",
    "ts-node": "^10.9.1",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.0.4",
    "vite-plugin-typejuice": "^0.0.1"
  },
  "scripts": {
    "dev": "yarn dev.ts",
    "docgen": "typedoc --options ./typedoc.json",
    "build:swc": "yarn swc ./src --out-file ./dist/index.js --config-file ./.swcrc",
    "build:tsc": "yarn tsc",
    "publish": "yarn build:swc && yarn docgen && npm publish"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-rate-limit": "^1.3.0",
    "typedoc": "^0.24.7"
  }
}
